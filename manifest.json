{
    "manifest_version": 3,
    "name": "Canvas Chrome API Test",
    "version": "1.0.0",
    "description": "Testing Canvas API with Chrome Extensions",
    "permissions": ["storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "type": "module"
      }
    ]
}