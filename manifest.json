{
  "manifest_version": 3,
  "name": "Canvas Assignment Fetcher",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["popup/fetch.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
